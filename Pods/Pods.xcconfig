HEADER_SEARCH_PATHS = ${PODS_HEADERS_SEARCH_PATHS}
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
PODS_ROOT = ${SRCROOT}/Pods
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/BlocksKit" "${PODS_ROOT}/BuildHeaders/LRResty" "${PODS_ROOT}/BuildHeaders/MBProgressHUD" "${PODS_ROOT}/BuildHeaders/NSData+Base64" "${PODS_ROOT}/BuildHeaders/RGProgressHUD" "${PODS_ROOT}/BuildHeaders/libffi"
OTHER_LDFLAGS = -ObjC -Wl,-no_compact_unwind -framework CoreGraphics -framework MessageUI
ALWAYS_SEARCH_USER_PATHS = YES
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/BlocksKit" "${PODS_ROOT}/Headers/LRResty" "${PODS_ROOT}/Headers/MBProgressHUD" "${PODS_ROOT}/Headers/NSData+Base64" "${PODS_ROOT}/Headers/RGProgressHUD" "${PODS_ROOT}/Headers/libffi"